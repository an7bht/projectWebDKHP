
    <header id="wrap-header">
        <div class="container" style="background-color: white;">
            
            <div class="row">  
                <h2 class="text-center">CẬP NHẬT NHÂN VIÊN

                </h2>
            </div>
            <div class="form-group col-xs-12 col-sm-12 col-md-12 col-lg-12" style="margin-top: 2%;">
                <ul class="nav nav-tabs">
                    <li class="active"><a data-toggle="tab" href="#tab1">Danh sách nhân viên</a></li>
                    <li><a data-toggle="tab" href="#tab2">Thêm nhân viên</a></li>
                </ul>
                <div class="tab-content">
                    <div id="tab1" class="tab-pane fade in active">
                        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
    
                            <div class="row">
                                <table class="table table-bordered" style="margin-top: 2%;">
                                    <thead>
                                      <tr>
                                        <th>Mã nhân viên</th>
                                        <th>Họ và tên</th>
                                        <th>Địa chỉ</th>
                                        <th>Giới tính</th>
                                        <th>Ngày sinh</th>
                                        <th>Số điện thoại</th>
                                        <th>Mã khoa</th>
                                        <th></th>
                                      </tr>
                                    </thead>
                                    <tbody>
                                        <%for(let i=0;i< listnv.length;i++){%>
                                            <tr>
                                                <td>
                                                    <%=listnv[i].MaNV%>
                                                </td>
                                                <td>
                                                    <%=listnv[i].HoTen %>
                                                </td>
                                                <td>
                                                    <%=listnv[i].DiaChi%>
                                                </td>
                                                <td>
                                                    <%=listnv[i].GioiTinh%>
                                                </td>
                                                <td>
                                                    <%=listnv[i].NgaySinh.getDate().toString()%>-
                                                        <%=(listnv[i].NgaySinh.getMonth()+1).toString()%>-
                                                            <%=listnv[i].NgaySinh.getFullYear().toString()%>
                                                </td>
                                                
                                                <td>
                                                    <%=listnv[i].SoDT%>
                                                </td>
                                                <td>
                                                    <%=listnv[i].MaKhoa%>
                                                </td>
                                                <td>
                                                    <a href="/nhanvien/cnnhanvien/editnv/<%=listnv[i].MaNV%>"
                                                        class="btn btn-warning">Sửa</a>
                                                    <a href="/nhanvien/cnnhanvien/xoanv/<%=listnv[i].MaNV%>"
                                                        class="btn btn-danger ">Xóa</a>
                                                    <a href= "/nhanvien/cnnhanvien/datlaimksv/<%=listnv[i].MaNV%>"
                                                        class="btn btn-danger ">Đặt lại mật khẩu</a>
                                                </td>
                                            </tr>
                                            <%}%>
                                    </tbody>
                                  </table>
                            </div>
                        </div>
                    </div>
                    <div id="tab2" class="tab-pane fade">
                        </br>
                            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-6">
                                    <h4>Thêm nhân viên</h4>
                                    <a href="/nhanvien/cnnhanvien/add-nv" class="btn btn-primary" >Thêm nhân viên</a><br />
                                </div>
                            </div>
                    </div>
                </div>
            </div>  
        </div>
        <script>

        </script>
    </header>
