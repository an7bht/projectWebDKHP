
    <div class="container">
        <div class="row col-xs-12 col-sm-12 col-md-12 col-lg-12" style="text-align: center;">
            <h2>Đổi mật khẩu</h2><br><br>
        </div>
        <form class="form-horizontal"  action="/sinhvien/doimatkhau" method="post" enctype="multipart/form-data">
            <div class="form-group">
                <label class="control-label col-lg-2" for="masv">Mã sinh viên:</label>
                <div class="col-lg-8">
                    <input type="text" class="form-control" id="masv" value=<%= mssv%> readonly >
                </div>
                <div class="col-lg-2"></div>
            </div>

            

            <div class="form-group">
                <label class="control-label col-lg-2" for="mkc">Mật khẩu cũ:</label>
                <div class="col-lg-8">
                    <input type="password" class="form-control"  placeholder="Mật khẩu cũ" name="mkc" value=<%= mkc%> >
                </div>
                <label class="control-label col-lg-2" for="erroe"><%= erromkc%></label>  
                <div class="col-lg-2"></div>
            </div>
          

            <div class="form-group">
                <label class="control-label col-lg-2" for="mkm">Mật khẩu mới:</label>
                <div class="col-lg-8">
                    <input type="password" class="form-control"  placeholder="Mật khẩu mới" name="mkm" value=<%= mkm%> >
                </div>
                <div class="col-lg-2"></div>
                <label class="control-label col-lg-2" for="erroe"><%= erromkm%></label>  
            </div>
            <div class="form-group">
                <div class="col-sm-offset-2 col-sm-10">
                    <button type="submit" class="btn btn-default">Cập nhật</button>
                </div>
            </div>
        </form>
    </div>
  